pipeline {
    agent any
    tools {
        maven '$M2_HOME'
    }
    stages {
        stage('GIT') {
            steps {
                git branch: 'master',
                    url: 'https://github.com/HOUDA-18/timesheetprojectDevops.git'
            }
        }
        
         
        stage('Compile') {
            steps {
                sh 'mvn clean compile'
                
            }
        }
           
  
     
    }
}
