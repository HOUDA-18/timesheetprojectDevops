pipeline {
    agent any

    stages {
        stage('Git') {
            steps {
                git branch : 'master ',
                url : 'https://github.com/hwafa/timesheetproject.git'
            }
        }
         stage('Compile') {
            steps {
                sh 'mvn clean compile'
            }
        }
        stage('SonarQube Analysis') {

steps{

script {

def scannerHome = tool 'sonar'

withSonarQubeEnv {

sh "${scannerHome}/bin/sonar-scanner"

}

}

}

}


    }
}
